INCLUDES= -I ./ -I ./helpers
OBJECTS= ../../../programsapi/bin/crt032.o ../../../programsapi/bin/system32.o ../../../programsapi/bin/atoll32.o ../../../programsapi/bin/isalpha32.o ../../../programsapi/bin/tolower32.o ../../../programsapi/bin/fputc32.o ../../../programsapi/bin/vsprintf32.o ../../../programsapi/bin/strtol32.o ../../../programsapi/bin/strcpy32.o ../../../programsapi/bin/realloc32.o ../../../programsapi/bin/putsc32.o ../../../programsapi/bin/puts32.o ../../../programsapi/bin/hang32.o ../../../programsapi/bin/__stack_chk_fail32.o ../../../programsapi/bin/write32.o ../../../programsapi/bin/convert32.o ../../../programsapi/bin/vsnprintf32.o  ../../../programsapi/bin/strlen32.o ../../../programsapi/bin/vfprintf32.o ../../../programsapi/bin/fprintf32.o ../../../programsapi/bin/printf32.o ../../../programsapi/bin/sprintf32.o ../../../programsapi/bin/putchar32.o ../../../programsapi/bin/realpath32.o ../../../programsapi/bin/fclose32.o ../../../programsapi/bin/fread32.o ../../../programsapi/bin/fopen32.o ../../../programsapi/bin/ungetc32.o ../../../programsapi/bin/strncpy32.o ../../../programsapi/bin/memset32.o ../../../programsapi/bin/malloc32.o ../../../programsapi/bin/calloc32.o ../../../programsapi/bin/strcmp32.o ../../../programsapi/bin/assert32.o ../../../programsapi/bin/free32.o ../../../programsapi/bin/memcpy32.o ../../../programsapi/bin/getc32.o ../../../programsapi/bin/exit32.o ./build/misc.o ./build/lexer.o  ./build/lex_process.o ./build/token.o ./build/expressionable.o ./build/parser.o ./build/validator.o ./build/symresolver.o ./build/scope.o ./build/resolver.o ./build/rdefault.o ./build/helper.o ./build/codegen.o ./build/helpers/vector.o ./build/helpers/buffer.o ./build/helpers/hashmap.o ./build/compiler.o ./build/cprocess.o ./build/preprocessor/preprocessor.o ./build/preprocessor/native.o ./build/array.o ./build/node.o ./build/preprocessor/static-includes.o ./build/preprocessor/static-includes/stddef.o ./build/preprocessor/static-includes/stdarg.o  ./build/fixup.o ./build/native.o ./build/stackframe.o
all: ${OBJECTS}
	${CC} main.c -ffreestanding -nostdlib -T ../../../programsapi/linker32.ld -o ../../../bin/programs/32bit/dragon.bin ${OBJECTS} -g

